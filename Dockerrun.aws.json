{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "php:8.3-fpm",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "80"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current/storage",
      "ContainerDirectory": "/var/www/html/storage"
    }
  ],
  "Logging": "/var/log/nginx"
}
